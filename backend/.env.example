# Frontend application URL for CORS configuration
FRONTEND_URL=http://localhost:3000

# JWT Authentication (generate with: openssl rand -hex 32)
SECRET_KEY=your-secret-key-change-in-production-use-openssl-rand-hex-32

# MongoDB Configuration
# For local development:
# MONGODB_URL=mongodb://localhost:27017
# MONGODB_DATABASE=qubitkitdev

# For MongoDB Atlas:
# MONGODB_URL=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/?retryWrites=true&w=majority
# MONGODB_DATABASE=qubitkit

# Log file path for persistent storage (optional)
# Relative to project root. Creates directory if needed
LOG_FILE_PATH=logs/app.log

# SQUANDER SSH server
SQUANDER_SSH_HOST=
SQUANDER_SSH_USER=
SQUANDER_SSH_PATH=
SQUANDER_EXEC_TIMEOUT=300
SSH_KEY_PATH=
SSH_TIMEOUT=30